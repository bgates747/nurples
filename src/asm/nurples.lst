0507FC 00 00 00    0015*  laser_vel:              dl 0x000000 ; 3 bytes not currently used
0507FF 00 00 00    0016*  laser_heading:          dl 0x000000 ; 3 bytes sprite movement direction deg256 16.8 fixed
050802 00 00 00    0017*  laser_orientation:      dl 0x000000 ; 3 bytes not currently used
050805 00          0018*  laser_animation:        db 0x00 ; 1 bytes current sprite animation frame
050806 00          0019*  laser_animation_timer:  db 0x00 ; 1 bytes decremented every frame, when zero, advance animation
050807 00          0020*  laser_move_timer:       db 0x00 ; 1 bytes not currently used
050808 00          0021*  laser_move_step:        db 0x00 ; 1 bytes not currently used
050809 00          0022*  laser_points:           db 0x00 ; 1 bytes not currently used
05080A 00          0023*  laser_shield_damage:    db 0x00 ; 1 bytes not currently used
05080B             0024*  laser_end_variables: ; for when we want to traverse this table in reverse
