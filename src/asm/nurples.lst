053A6D FD 2F 0E    0222*      ld (iy+sprite_y),hl
053A70             0223*  ; reverse y-axis velocity
053A70 FD 27 14    0224*      ld hl,(iy+sprite_yvel)
053A73 CD A4 06 04 0225*      call neg_hlu
053A77 FD 2F 14    0226*      ld (iy+sprite_yvel),hl
053A7A             0227*  ; go check for x-axis collisions
053A7A C3 3F 3A 05 0228*      jp @xaxis
