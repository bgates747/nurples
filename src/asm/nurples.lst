041AEC             0015*  
041AEC             0016*  ; this routine detects collisions between two collidable objects,
041AEC             0017*  ; which can include sprites, active tiles, or any screen coordinate
041AEC             0018*  ; which has been set up with a sprite table structure that includes dim_x
041AEC             0019*  ; representing the radius of the collidable object
041AEC             0020*  ; inputs: ix and iy point to the colliding objects' sprite table records
041AEC             0021*  ; outputs: uh.l = distance - sum of radii
041AEC             0022*  ;          zero flag set or sign flag negative if collision detected
041AEC             0023*  ;          dx168 and dy168 are also populated with the signed displacements
041AEC             0024*  ; destroys: everything except indexes
041AEC             0025*  check_collision:
041AEC             0026*  ; compute dx = x1-x0
041AEC DD 27 0B    0027*      ld hl,(ix+sprite_x)
041AEF FD 17 0B    0028*      ld de,(iy+sprite_x)
041AF2 AF          0029*      xor a ; clear carry
041AF3 ED 52       0030*      sbc hl,de
041AF5 22 A2 09 04 0031*      ld (dx168),hl ; dx to scratch
041AF9             0032*  ; compute dy = y1-y0
041AF9 DD 27 0E    0033*      ld hl,(ix+sprite_y)
041AFC FD 17 0E    0034*      ld de,(iy+sprite_y)
041AFF AF          0035*      xor a ; clear carry
041B00 ED 52       0036*      sbc hl,de ; hl = dy
041B02 22 A8 09 04 0037*      ld (dy168),hl ; dy to scratch
041B06             0038*  ; compute euclidean distance between centers
041B06 CD AE 09 04 0039*      call distance168 ; uh.l = distance
041B0A             0040*  ; check for overlap by summing half the radii
041B0A DD 7E 09    0041*      ld a,(ix+sprite_dim_x)
041B0D CB 3F       0042*      srl a ; divide by 2
041B0F FD 5E 09    0043*      ld e,(iy+sprite_dim_x)
041B12 CB 3B       0044*      srl e ; divide by 2
041B14 83          0045*      add a,e
041B15 11 00 00 00 0046*      ld de,0 ; clear deu and e
041B19 57          0047*      ld d,a ; ud.e = sum of radii
041B1A B7          0048*      or a ; clear carry
041B1B ED 52       0049*      sbc hl,de ; uh.l = distance - sum of radii
041B1D C9          0050*      ret
041B1E             0051*  ; end check_collision
