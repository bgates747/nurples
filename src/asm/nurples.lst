053A24 AF          0192*      xor a ; reset screen edge collision flag and clear carry
053A25 32 5B 39 05 0193*      ld (sprite_screen_edge),a
053A29             0194*  ; y-axis first since we already have it in hl
053A29 11 00 00 00 0195*      ld de,-sprite_top*256
