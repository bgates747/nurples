041B48             0022*  ; initialize pointers and loop counter
041B48 FD 21 18 77 0023*      ld iy,table_base ; set iy to first record in table
       04          
041B4D 06 10       0024*      ld b,table_max_records ; loop counter
041B4F             0025*  @move_loop:
