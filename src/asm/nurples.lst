0507E6 00          0005*  laser_type:             db 0x00 ; 1 bytes currently not used
0507E7 08 01 00    0006*  laser_base_bufferId:    dl BUF_LASER_A ; 3 bytes bitmap bufferId
0507EA 00 00 00    0007*  laser_move_program:     dl 0x000000 ; 3 bytes not currently used
0507ED 00          0008*  laser_collisions:       db 0x00 ; 1 bytes uses sprite_alive and sprite_just_died flags
0507EE 00          0009*  laser_dim_x:            db 0x00 ; 1 bytes sprite width in pixels
0507EF 00          0010*  laser_dim_y:            db 0x00 ; 1 bytes sprite height in pixels
0507F0 00 00 00    0011*  laser_x:                dl 0x000000 ; 3 bytes 16.8 fractional x position in pixels
0507F3 00 00 00    0012*  laser_y:                dl 0x000000 ; 3 bytes 16.8 fractional y position in pixels
0507F6 00 00 00    0013*  laser_xvel:             dl 0x000000 ; 3 bytes x-component velocity, 16.8 fixed, pixels
