041AEC             0001*  ; ====== COLLISION DETECTION ======
041AEC             0002*  ; --- constants ---
041AEC             0003*  ; sprite_collisions flags
041AEC             0004*  collides_with_player: equ %00000001
041AEC             0005*  collides_with_laser: equ %00000010
