053A7E 3A 5B 39 05 0230*      ld a,(sprite_screen_edge)
053A82 F6 02       0231*      or a,collide_left
053A84 21 00 00 00 0232*      ld hl,sprite_left*256
053A88 C3 96 3A 05 0233*      jp @collide_x
053A8C             0234*  @right:
053A8C 3A 5B 39 05 0235*      ld a,(sprite_screen_edge)
053A90 F6 01       0236*      or a,collide_right
053A92 21 00 EF 00 0237*      ld hl,sprite_right*256
053A96             0238*  @collide_x:
053A96 32 5B 39 05 0239*      ld (sprite_screen_edge),a
053A9A FD 2F 0B    0240*      ld (iy+sprite_x),hl
053A9D             0241*  ; reverse x-axis velocity
053A9D FD 27 11    0242*      ld hl,(iy+sprite_xvel)
053AA0 CD A4 06 04 0243*      call neg_hlu
053AA4 FD 2F 11    0244*      ld (iy+sprite_xvel),hl
053AA7             0245*  ; final collision flag to a and return
053AA7 3A 5B 39 05 0246*      ld a,(sprite_screen_edge)
