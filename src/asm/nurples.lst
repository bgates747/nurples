041DCF DD 66 06    0084*       ld h,(ix+sprite_y+2)        ; Load ay integer part high byte
041DD2 11 00 00 00 0085*       ld de,0                     ; Clear deu and d
041DD6 DD 5E 1F    0086*       ld e,(ix+sprite_dim_y)      ; Load ah
