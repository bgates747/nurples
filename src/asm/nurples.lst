0453E1 CD A4 06 04 0225*      call neg_hlu
0453E5 FD 2F 14    0226*      ld (iy+sprite_yvel),hl
0453E8             0227*  ; go check for x-axis collisions
0453E8 C3 AD 53 04 0228*      jp @xaxis
