041C6B             0367*  ; VDU 23,0,&A0,targetBufferID%;24,4,4;
041C6B 17 00 A0    0368*      db 23,0,0xA0
041C6E 00 00       0369*  @bufferId2:    dw 0x0000 ; targetBufferId
