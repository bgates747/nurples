041307             0096*  ; disable timer
041307 3E 06       0097*      ld a,PRT_IRQ_0 | IRQ_EN_0 | PRT_MODE_0 | CLK_DIV_16 | RST_EN_1 | PRT_EN_0
041309 ED 39 83    0098*  	out0 ($83),a
