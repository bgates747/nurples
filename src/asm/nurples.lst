040060             0038*  ; destroys: af
040060             0039*      MACRO HLU_TO_A
040060             0040*      push hl ; 4 cycles
040060             0041*      inc sp ; 1 cycle
040060             0042*      pop af ; 4 cycles
040060             0043*      dec sp ; 1 cycle
