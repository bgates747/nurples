053A42 11 00 00 00 0204*      ld de,-sprite_left*256
053A46 ED 5A       0205*      adc hl,de
053A48 FA 7E 3A 05 0206*      jp m,@left
053A4C ED 52       0207*      sbc hl,de ; hl back to original value
053A4E 11 00 11 FF 0208*      ld de,-sprite_right*256
053A52 ED 5A       0209*      adc hl,de
053A54 F2 8C 3A 05 0210*      jp p,@right
