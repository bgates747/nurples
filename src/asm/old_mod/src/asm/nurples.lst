04582E 7C          0308*      ld a,h
04582F 32 47 58 04 0309*      ld (@green),a
045833 7D          0310*      ld a,l
045834 32 48 58 04 0311*      ld (@blue),a
045838 21 43 58 04 0312*  	ld hl,@cmd
04583C 01 06 00 00 0313*  	ld bc,@end-@cmd
