04093E 0E          0355*      db 14 ; consolidate blocks
04093F             0356*  ; the "expand bitmap" command is:
04093F             0357*  ; VDU 23,0,&A0,targetBufferID%;&48,2,sourceBufferId%;0,&7F,&BF,&FF
04093F 17 00 A0    0358*      db 23,0,0xA0
