0409DF 3E 48       0320*      ld a,0x48
0409E1 32 0C 0A 04 0321*      ld (@bufferId0+2),a
0409E5 3E 17       0322*      ld a,23
0409E7 32 22 0A 04 0323*      ld (@bufferId1+2),a
0409EB 3E 18       0324*      ld a,24
0409ED 32 19 0A 04 0325*      ld (@bufferId2+2),a
