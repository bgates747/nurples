040A06 0E          0338*      db 14 ; consolidate blocks
040A07             0339*  ; the "expand bitmap" command is:
040A07             0340*  ; VDU 23,0,&A0,targetBufferID%;&48,2,sourceBufferId%;0,&7F,&BF,&FF
040A07 17 00 A0    0341*      db 23,0,0xA0
