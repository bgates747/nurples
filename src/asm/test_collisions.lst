042304 AF          0170*       xor a ; clear carry
042305 ED 52       0171*       sbc hl,de
042307 22 2E 08 04 0172*       ld (dx168),hl ; dx to scratch
04230B             0173*   ; compute dy = y1-y0
04230B DD 27 04    0174*       ld hl,(ix+sprite_y)
04230E FD 17 04    0175*       ld de,(iy+sprite_y)
042311 AF          0176*       xor a ; clear carry
042312 ED 52       0177*       sbc hl,de ; hl = dy
042314 22 34 08 04 0178*       ld (dy168),hl ; dy to scratch
042318             0179*   ; compute euclidean distance between centers
042318 CD 3A 08 04 0180*       call distance168 ; uh.l = distance
04231C             0181*   ; check for overlap by summing half the radii
04231C DD 7E 1E    0182*       ld a,(ix+sprite_dim_x)
04231F CB 3F       0183*       srl a ; divide by 2
042321 FD 5E 1E    0184*       ld e,(iy+sprite_dim_x)
042324 CB 3B       0185*       srl e ; divide by 2
042326 83          0186*       add a,e
042327 11 00 00 00 0187*       ld de,0 ; clear deu and e
04232B 57          0188*       ld d,a ; ud.e = sum of radii
04232C B7          0189*       or a ; clear carry
04232D ED 52       0190*       sbc hl,de ; uh.l = distance - sum of radii
04232F C8          0191*       ret z ; return collision
042330 F0          0192*       ret p ; return no collision
042331 AF          0193*       xor a ; return collision
042332 C9          0194*       ret
