0422EC 19          0137*       add hl,de                   ; (bx + bw)
0422ED DD 5E 02    0138*       ld e,(ix+sprite_x+1)        ; Load ax integer part low byte
0422F0 DD 56 03    0139*       ld d,(ix+sprite_x+2)        ; Load ax integer part high byte
