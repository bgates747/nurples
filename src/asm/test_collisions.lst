047600             0003*   ; compute orientation to player
047600             0004*   ; based on relative positions
047600             0005*   ; returns: uh.l 16.8 fixed angle256 to player
047600             0006*   ;    ub.c and ud.e as 16.8 signed fixed point numbers
047600             0007*   ;    representing delta-x/y *to* target respectively
047600             0008*   ; inputs: iy pointed at sprite record
047600             0009*   ; destroys: everything except iy (sprite record pointer)
047600             0010*   orientation_to_player:
047600 DD 21 03 92 0011*       ld ix,player_begin
       04          
047605             0012*   ; compute dx
047605 DD 27 01    0013*       ld hl,(ix+sprite_x)
047608 FD 17 01    0014*       ld de,(iy+sprite_x)
04760B B7          0015*       or a ; clear carry
04760C ED 52       0016*       sbc hl,de ; dx
04760E E5          0017*       push hl ; save dx
04760F             0018*   ; compute dy
04760F DD 27 04    0019*       ld hl,(ix+sprite_y)
047612 FD 17 04    0020*       ld de,(iy+sprite_y)
047615 B7          0021*       or a ; clear carry
047616 ED 52       0022*       sbc hl,de ; dy
047618 EB          0023*       ex de,hl ; de = dy
047619             0024*   ; compute angle to player
047619 C1          0025*       pop bc ; dx (was hl)
04761A C5          0026*       push bc ; save dx
04761B D5          0027*       push de ; save dy
04761C CD 9A 08 04 0028*       call atan2_168fast ; uh.l = angle to player
047620 3E E0       0029*       ld a,%11100000
047622 A5          0030*       and l ; round angle to nearest 32th of a circle
047623 6F          0031*       ld l,a
047624 22 52 76 04 0032*       ld (Bearing_t),hl ; return angle to player
047628 D1          0033*       pop de ; restore dy
047629 C1          0034*       pop bc ; restore dx
04762A C9          0035*       ret
