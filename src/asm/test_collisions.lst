04227E             0081*   ; check_collision:
04227E             0082*   ; Detects collision using bounding box method.
04227E             0083*   ; Inputs:
04227E             0084*   ;   IX -> Sprite/Tile table for Collider A
04227E             0085*   ;   IY -> Sprite/Tile table for Collider B
04227E             0086*   ; Outputs:
04227E             0087*   ;   a = 0, zero flag set, positive sign, carry reset -> Collision detected
04227E             0088*   ;   a = -1, zero flag reset, negative sign, carry set -> No collision detected
04227E             0089*   ; Destroys: HL, DE, AF
04227E             0090*   ; check_collision:
04227E             0091*   check_collision_box:
04227E             0092*   ; Test 1: (ay + ah) - by < 0
04227E 21 00 00 00 0093*       ld hl,0                     ; Clear hlu
042282 DD 6E 05    0094*       ld l,(ix+sprite_y+1)        ; Load ay integer part low byte
042285 DD 66 06    0095*       ld h,(ix+sprite_y+2)        ; Load ay integer part high byte
042288 11 00 00 00 0096*       ld de,0                     ; Clear deu and d
04228C DD 5E 1F    0097*       ld e,(ix+sprite_dim_y)      ; Load ah
04228F 19          0098*       add hl,de                   ; (ay + ah)
042290 FD 5E 05    0099*       ld e,(iy+sprite_y+1)        ; Load by integer part low byte
042293 FD 56 06    0100*       ld d,(iy+sprite_y+2)        ; Load by integer part high byte
042296 AF          0101*       xor a                       ; Clear carry
042297 ED 52       0102*       sbc hl,de                   ; (ay + ah) - by
042299 FA FC 22 04 0103*       jp m,@not_collision         ; No collision if negative
04229D             0104*   
04229D             0105*   ; Test 2: ay - (by + bh) < 0
04229D 21 00 00 00 0106*       ld hl,0                     ; Clear hlu
0422A1 FD 6E 05    0107*       ld l,(iy+sprite_y+1)        ; Load by integer part low byte
0422A4 FD 66 06    0108*       ld h,(iy+sprite_y+2)        ; Load by integer part high byte
0422A7 11 00 00 00 0109*       ld de,0                     ; Clear deu and d
0422AB FD 5E 1F    0110*       ld e,(iy+sprite_dim_y)      ; Load bh
0422AE 19          0111*       add hl,de                   ; (by + bh)
0422AF DD 5E 05    0112*       ld e,(ix+sprite_y+1)        ; Load ay integer part low byte
0422B2 DD 56 06    0113*       ld d,(ix+sprite_y+2)        ; Load ay integer part high byte
0422B5 AF          0114*       xor a                       ; Clear carry
0422B6 ED 52       0115*       sbc hl,de                   ; (by + bh) - ay
0422B8 FA FC 22 04 0116*       jp m,@not_collision         ; No collision if negative
0422BC             0117*   
0422BC             0118*   ; Test 3: (ax + aw) - bx < 0
0422BC 21 00 00 00 0119*       ld hl,0                     ; Clear hlu
0422C0 DD 6E 02    0120*       ld l,(ix+sprite_x+1)        ; Load ax integer part low byte
0422C3 DD 66 03    0121*       ld h,(ix+sprite_x+2)        ; Load ax integer part high byte
0422C6 11 00 00 00 0122*       ld de,0                     ; Clear deu and d
0422CA DD 5E 1E    0123*       ld e,(ix+sprite_dim_x)      ; Load aw
0422CD 19          0124*       add hl,de                   ; (ax + aw)
0422CE FD 5E 02    0125*       ld e,(iy+sprite_x+1)        ; Load bx integer part low byte
0422D1 FD 56 03    0126*       ld d,(iy+sprite_x+2)        ; Load bx integer part high byte
0422D4 AF          0127*       xor a                       ; Clear carry
0422D5 ED 52       0128*       sbc hl,de                   ; (ax + aw) - bx
0422D7 FA FC 22 04 0129*       jp m,@not_collision         ; No collision if negative
0422DB             0130*   
0422DB             0131*   ; Test 4: ax - (bx + bw) < 0
