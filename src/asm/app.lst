040077             0126*  ; HL: Pointer to string
040077             0127*  printString:
040077 C5          0128*  	PUSH	BC
040078 01 00 00 00 0129*  	LD		BC,0
04007C 3E 00       0130*  	LD 	 	A,0
04007E 5B DF       0131*  	RST.LIL 18h
040080 C1          0132*  	POP		BC
040081 C9          0133*  	RET
